{
  "manifest_version": 2,
  "name": "Lockt In",
  "version": "1.0",
  "description": "A minimal pomodoro timer for Firefox.",
  "icons": {
    "48": "assets/logo.svg"
  },
  "permissions": ["activeTab", "storage"],
  "browser_action": {
    "default_icon": "assets/logo.svg",
    "default_title": "Lockt In",
    "default_popup": "lockt-in/index.html"
  }
}
