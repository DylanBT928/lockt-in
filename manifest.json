{
  "manifest_version": 2,
  "name": "ZenPomo",
  "version": "1.0",
  "description": "A minimal pomodoro timer for Firefox.",
  "icons": {
    "48": "assets/logo.svg"
  },
  "permissions": ["activeTab"],
  "browser_action": {
    "default_icon": "assets/logo.svg",
    "default_title": "ZenPomo",
    "default_popup": "zen-pomo/index.html"
  }
}
